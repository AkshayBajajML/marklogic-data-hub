{
    "extends": [
        "defaults/rules/eslint/es6/airbnb"
    ],
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "expect": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "no-unused-expressions": 0
    }
}